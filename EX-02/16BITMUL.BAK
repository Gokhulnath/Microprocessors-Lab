
;Program for multiplying 2, 16 bit numbers

assume cs:code,ds:data
data segment 
	opr1 dw 0011h
        opr2 dw 0099h
        result1 dw 0000h
	result2 dw 0000h
data ends
code segment
        org 0100h
start:  mov ax,data
        mov ds,ax
        mov ax,0000h
        mov ax,opr2
        mov bx,opr1
	MUL bx
	mov result1,dx
	mov result2,ax
        mov ah,4ch
        int 21h
	code ends
end start
